<html>
  <head>
    <title><%= title %></title>
  </head>
  <body>
    <h1><%= title %></h1>
    <% if (locals.genres) { %>
    <ul>
      <% genres.forEach(genre => { %>
      <li>
        <a href=<%= `/genre/${genre.genre_id}` %>><button><%= genre.genre %></button></a>
      </li>
      <% }); %>
    </ul>
    <% } %>
  </body>
</html>
